function logch() {
	const username = "";
	a = ["/logined", "/edit"];
	b = ["/log", "/sign"];
	c = location.href;
	arr = c.split("/");
	c = "/" + arr[arr.length - 1];
	console.log(c);
	login = 0; //預設未登入
	$.ajax({
		url: '/loginch',
		type: 'POST',
		data: {
			username: username
		},
		success: function(data) {
			if (data === "success") {
				// 登入成功
				login = 1;
			} else {
				login = 0;
			}
			
			if (a.includes(c)) {

				if (login < 1) {
					
					location.href = b[0];
				} 
			} else if (b.includes(c)) {

				if (login > 0) {
					
					location.href = a[0];
				} 
			} else {  }
		}
	});


}